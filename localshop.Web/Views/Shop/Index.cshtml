                        <!-- List style 1 -->
                        <div id="shop-1" class="tab-pane active">
                            <div class="row">
                                @foreach (var product in Model.Products)
                                {
                                    <div class="col-xl-4 col-lg-6 col-md-6 col-sm-6">
                                        <div class="product-wrap product-border-2 pro-hover-shadow mb-40">
                                            <div class="product-img mb-15">
                                                @if (product.Status == StatusNames.OutOfStock)
                                                {
                                                    <span class="new-stock"><span>Stock <br>Out</span></span>
                                                }
                                                else if (product.Product.DiscountPrice != null)
                                                {
                                                    var percetage = 100 - (product.Product.DiscountPrice * 100 / product.Product.Price);
                                                    if (percetage >= 15)
                                                    {
                                                        if (product.Product.EndDiscountDate != null)
                                                        {
                                                            if (DateTime.Now <= product.Product.EndDiscountDate.Value)
                                                            {
                                                                <span class="price-dec">-@Math.Round(percetage.Value)%</span>
                                                            }
                                                            else if ((DateTime.Now - product.Product.DateAdded).TotalDays <= 7)
                                                            {
                                                                <span class="price-dec font-dec">NEW</span>
                                                            }
                                                        }
                                                        else
                                                        {
                                                            <span class="price-dec">-@Math.Round(percetage.Value)%</span>
                                                        }
                                                    }
                                                    else if ((DateTime.Now - product.Product.DateAdded).TotalDays <= 7)
                                                    {
                                                        <span class="price-dec font-dec">NEW</span>
                                                    }
                                                }
                                                else if ((DateTime.Now - product.Product.DateAdded).TotalDays <= 7)
                                                {
                                                    <span class="price-dec font-dec">NEW</span>
                                                }
                                                <a href="@Url.Action("detail", "product", new { metaTitle=product.Product.MetaTitle })"><img src="@(product.Product.Images.FirstOrDefault()??ImageLinks.BrokenProductImage)" alt="@product.Product.Name"></a>
                                                <div class="product-action-2">
                                                    <a title="Compare" class="add-to-compare" href="javascript:void(0)" data-productid="@product.Product.Id"><i class="la la-repeat"></i></a>
                                                    @*<a data-toggle="modal" data-target="#productModal" title="Quick View" href="javascript:void(0)"><i class="la la-search"></i></a>*@
                                                    <a title="Wishlist" class="add-to-wishlist" href="javascript:void(0)" data-productid="@product.Product.Id"><i class="la la-heart-o"></i></a>
                                                    @if (product.Status != StatusNames.OutOfStock)
                                                    {
                                                        <a title="Add To Cart" class="add-to-cart" href="javascript:void(0)" data-productid="@product.Product.Id"><i class="la la-cart-plus"></i></a>
                                                    }
                                                </div>
